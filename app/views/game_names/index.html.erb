<p id="notice"><%= notice %></p>

<h1>Game Names</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @game_names.each do |game_name| %>
      <tr>
        <td><%= game_name.name %></td>
        <td><%= link_to 'Show', game_name %></td>
        <td><%= link_to 'Edit', edit_game_name_path(game_name) %></td>
        <td><%= link_to 'Destroy', game_name, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Game Name', new_game_name_path %>
