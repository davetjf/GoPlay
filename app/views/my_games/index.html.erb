<div id="conversation" class="w3-container">

<h1>My Games</h1>

<table>
  <thead>
    <tr>
      <th>Game name</th>
      <th>Game type</th>
      <th>Date</th>
      <th>Player one</th>
      <th>Player two</th>
      <th>Player three</th>
      <th>Player four</th>
      <th>Isprivate</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @my_games.each do |my_game| %>
      <tr>
        <td><%= my_game.game_name %></td>
        <td><%= my_game.game_type %></td>
        <td><%= my_game.date %></td>
        <td><%= my_game.player_one %></td>
        <td><%= my_game.player_two %></td>
        <td><%= my_game.player_three %></td>
        <td><%= my_game.player_four %></td>
        <td><%= my_game.isPrivate %></td>
        <td><%= link_to 'Show', my_game %></td>
        <td><%= link_to 'Edit', edit_my_game_path(my_game) %></td>
        <td><%= link_to 'Destroy', my_game, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New My Game', new_my_game_path %>
</div>